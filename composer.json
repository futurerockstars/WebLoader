{
	"name": "janmarek/webloader",
	"description": "Tool for loading or deploying CSS and JS files into web pages",
	"keywords": ["webloader", "css", "javascript", "assets", "nette"],
	"homepage": "http://addons.nette.org/cs/webloader",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Marek",
			"email": "mail@janmarek.net"
		}
	],
	"autoload": {
		"psr-4": {
			"WebLoader\\": "src"
		}
	},
	"require": {
		"nette/application": "~2.3@dev",
		"nette/di": "~2.3@dev",
		"nette/utils": "~2.3@dev"
	},
	"suggest": {
		"oyejorge/less.php": "LESS compiler written in PHP.",
		"leafo/scssphp": "SCSS compiler written in PHP.",
		"joseki/webloader-filters": "CSSMin & JSMin filters written in PHP.",
		"coffeescript/coffeescript": "CoffeeScript compiler written in PHP."
	},
	"require-dev": {
		"nette/application": "~2.3@dev",
		"nette/bootstrap": "~2.3@dev",
		"nette/caching": "~2.3@dev",
		"nette/component-model": "~2.2@dev",
		"nette/database": "~2.3@dev",
		"nette/deprecated": "~2.2@dev",
		"nette/di": "~2.3@dev",
		"nette/finder": "~2.3@dev",
		"nette/forms": "~2.3@dev",
		"nette/http": "~2.3@dev",
		"nette/mail": "~2.3@dev",
		"nette/neon": "~2.3@dev",
		"nette/php-generator": "~2.3@dev",
		"nette/reflection": "~2.3@dev",
		"nette/robot-loader": "~2.3@dev",
		"nette/safe-stream": "~2.3@dev",
		"nette/security": "~2.3@dev",
		"nette/tokenizer": "~2.2@dev",
		"nette/utils": "~2.3@dev",
		"latte/latte": "~2.3@dev",
		"tracy/tracy": "~2.3@dev",

		"oyejorge/less.php": "~1.5",
		"leafo/scssphp": "~0.1",
		"coffeescript/coffeescript": "1.3.*",

		"mockery/mockery": "^1.6.2",
		"phpunit/phpunit": "^9.5.0",
		"brianium/paratest": "^6.10",
		"orisai/coding-standard": "^3.7",
		"phpstan/extension-installer": "^1.3",
		"phpstan/phpstan": "^1.10",
		"phpstan/phpstan-deprecation-rules": "^1.1",
		"phpstan/phpstan-nette": "^1.2",
		"phpstan/phpstan-phpunit": "^1.3",
		"phpstan/phpstan-strict-rules": "^1.5",
		"staabm/annotate-pull-request-from-checkstyle": "^1.8"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.4-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
